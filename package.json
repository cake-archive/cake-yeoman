{
  "name": "generator-cake",
  "version": "0.2.3",
  "description": "Yeoman generator for Cake build scripts",
  "homepage": "https://github.com/agc93/generator-cake/",
  "author": {
    "name": "Alistair Chapman",
    "email": "alistair@agchapman.com",
    "url": "https://agchapman.com"
  },
  "preferGlobal": true,
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman-generator",
    "cake-build",
    "build",
    "csharp"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "yeoman-generator": "^0.23.0",
    "yeoman-remote": "^1.0.1",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "yeoman-test": "^1.0.0",
    "yeoman-assert": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-config-xo": "^0.16.0",
    "eslint-config-xo-space": "^0.14.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^2.0.0",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.0.0",
    "gulp-plumber": "^1.0.0",
    "gulp-nsp": "^2.1.0",
    "gulp-coveralls": "^0.1.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    },
    "rules": {
      "linebreak-style": "off",
      "space-before-function-paren": "off",
      "indent": "warn"
    }
  },
  "repository": "agc93/generator-cake",
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp"
  },
  "license": "MIT",
  "config": {}
}
